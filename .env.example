# Environment Variables Template for Fastlane
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file with real credentials!

# App Store Connect API Configuration (Recommended)
# Get these from App Store Connect -> Users and Access -> API Keys
APP_STORE_CONNECT_API_KEY_ID=YOUR_API_KEY_ID_HERE
APP_STORE_CONNECT_ISSUER_ID=YOUR_ISSUER_ID_HERE
APP_STORE_CONNECT_API_KEY_CONTENT=YOUR_BASE64_ENCODED_API_KEY_CONTENT_HERE

# App Configuration
APP_IDENTIFIER=com.example.businesscard
TEAM_ID=YOUR_TEAM_ID_HERE
ITUNES_CONNECT_TEAM_ID=YOUR_ITUNES_CONNECT_TEAM_ID_HERE

# Optional: Skip git status check for CI/CD environments
# SKIP_GIT_CHECK=true

# TestFlight Configuration
# DEFAULT_TESTFLIGHT_GROUPS=Internal Testers,Beta Testers
# ENABLE_AUTO_RELEASE_NOTES=true